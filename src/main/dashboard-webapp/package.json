{
  "name": "dashboard-webapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "VUE_CLI_SERVICE_CONFIG_PATH=$PWD/$(grep  VUE_APP_CONFIG .env | cut -d '=' -f2) vue-cli-service serve",
    "build": "VUE_CLI_SERVICE_CONFIG_PATH=$PWD/$(grep  VUE_APP_CONFIG .env | cut -d '=' -f2) vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint --no-fix",
    "lint-fix": "vue-cli-service lint --fix",
    "prod": "vue-cli-service build --dest ../resources/META-INF/resources/",
    "watch": "vue-cli-service build --mode=development --watch --dest ../resources/META-INF/resources/",
    "cypress:prod": "cypress open",
    "cypress:dev": "cypress open --config-file cypress.dev.json "
  },
  "dependencies": {
    "@mdi/font": "^5.5.55",
    "apexcharts": "^3.25.0",
    "core-js": "^3.6.4",
    "dom-to-image": "^2.6.0",
    "intersection-observer": "^0.11.0",
    "leaflet": "^1.6.0",
    "leaflet.markercluster": "^1.4.1",
    "moment-timezone": "^0.5.33",
    "portal-vue": "^2.1.7",
    "tiptap-vuetify": "^2.24.0",
    "typeface-roboto": "0.0.75",
    "v-click-outside": "^3.1.2",
    "vue": "^2.6.11",
    "vue-apexcharts": "^1.6.0",
    "vue-i18n": "^8.24.4",
    "vue-native-websocket": "^2.0.15",
    "vue-notification-bell": "^0.9.12",
    "vue-router": "^3.1.6",
    "vue-snotify": "^3.2.1",
    "vue2-leaflet": "^2.5.2",
    "vue2-leaflet-markercluster": "^3.1.0",
    "vuetify": "^2.4.11",
    "vuex": "^3.6.2",
    "vuex-shared-mutations": "^1.0.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-e2e-cypress": "^4.3.1",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-plugin-router": "^4.3.1",
    "@vue/cli-plugin-unit-jest": "^4.3.1",
    "@vue/cli-service": "~4.3.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "1.0.0-beta.31",
    "axios": "^0.21.1",
    "babel-eslint": "^10.1.0",
    "cypress": "^6.5.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "prettier": "^1.19.1",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "vue-cli-plugin-axios": "0.0.4",
    "vue-cli-plugin-vuetify": "~2.0.5",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.3.0"
  }
}
